    "SQL":"UPDATE MP_Questions SET %VOTE%count = %VOTE%count + 1  WHERE ID = '%POLLID%';"
  },
  {
    "name":"getPollData",
    "params":["POLLID"],
    "SQL":"SELECT `ID`, `Acount`, `Bcount`, `Ccount`, `Dcount` FROM MP_Questions WHERE ID = \"%POLLID%\";"
  },
